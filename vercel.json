{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/index.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "scripts": {
      "build": "npm run vercel-build"
    }
  }
}
